# ISW Service - Production Environment
# Non-sensitive defaults - All sensitive values injected from K8s secrets

NODE_ENV=production

# Server
PORT=4000
HOST=0.0.0.0

# Socket Server (Plain - for Interswitch communication)
SOCKET_SERVER_PORT_PLAIN=8001
SOCKET_SERVER_IP=0.0.0.0

# Logging
JET_LOGGER_MODE=CONSOLE
JET_LOGGER_TIMESTAMP=TRUE
JET_LOGGER_FORMAT=LINE

# Redis Configuration (values injected from K8s secrets in production)
REDIS_HOST=
REDIS_PORT=6379
REDIS_PASSWORD=
