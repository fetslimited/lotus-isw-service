{
  "name": "tmscore",
  "version": "0.0.0",
  "scripts": {
    "build": "ts-node build.ts",
    "lint": "eslint . --ext .ts",
    "start": "node ./dist/index.js --env=production --tls-max-v1.2",
    "start:dev": "nodemon",
    "test": "nodemon --config ./spec/nodemon.json"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@daos": "dist/daos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/Server"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "max-len": [
        "error",
        {
          "code": 100
        }
      ],
      "no-console": 1,
      "no-extra-boolean-cast": 0,
      "@typescript-eslint/restrict-plus-operands": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/no-floating-promises": 0,
      "@typescript-eslint/no-unsafe-member-access": 0,
      "@typescript-eslint/no-unsafe-assignment": 0
    }
  },
  "eslintIgnore": [
    "src/public/",
    "build.ts"
  ],
  "dependencies": {
    "@pm2/io": "^4.3.5",
    "@types/buffer-xor": "^2.0.0",
    "@types/node-cron": "^3.0.0",
    "@types/xml2json": "^0.11.4",
    "axios": "^0.21.1",
    "buffer-xor": "^2.0.2",
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "crypto-ts": "^1.0.2",
    "date-fns": "^2.20.1",
    "dotenv": "^8.2.0",
    "elastic-apm-node": "^3.15.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.4.1",
    "http-status-codes": "^2.1.4",
    "iso8583-js": "^2.0.0",
    "iso_8583": "^2.6.5",
    "jet-logger": "^1.0.4",
    "jsonfile": "^6.1.0",
    "jspos": "0.0.11-RELEASE",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "mongoose": "^5.11.15",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.0",
    "redis": "^4.0.4",
    "xml-js": "^1.6.11",
    "xml2js": "^0.4.23",
    "xml2json-light": "^1.0.6"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^9.0.6",
    "@types/jasmine": "^3.6.3",
    "@types/jsonfile": "^6.0.0",
    "@types/mocha": "^8.2.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.25",
    "@types/supertest": "^2.0.10",
    "@types/xml2js": "^0.4.9",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "chai": "^4.3.0",
    "eslint": "^7.19.0",
    "find": "^0.3.0",
    "fs-extra": "^9.1.0",
    "jasmine": "^3.6.4",
    "mocha": "^8.3.0",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  }
}
